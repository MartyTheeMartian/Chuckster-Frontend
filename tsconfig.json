{
  "compilerOptions": {
    "baseUrl": "./src/",
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "strictNullChecks": true,
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
